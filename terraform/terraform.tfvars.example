# Copy this file to terraform.tfvars and fill in your values.
# terraform.tfvars is gitignored and will not be committed.

project_id      = ""    # GCP project ID (must be globally unique)
billing_account = ""    # GCP billing account ID (format: XXXXXX-XXXXXX-XXXXXX)
org_id          = ""    # GCP organization ID (leave empty if using folder_id)
folder_id       = ""    # GCP folder ID (leave empty if using org_id)
bucket_name       = ""    # Globally unique GCS bucket name for the pipeline
inbox_bucket_name = ""    # Globally unique GCS bucket name for raw file uploads

# Optional: Override defaults
# bq_location             = "US"           # BigQuery dataset location (default: US)
# region                  = "us-central1"  # Compute region for Cloud Run, etc.
# biglake_connection_id   = "biglake-iceberg"
# vertex_ai_connection_id = "vertex-ai-remote"
# spark_connection_id     = "spark-proc"
# firestore_database_name = "pipeline-state"
